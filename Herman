//https://dmoj.ca/problem/coci06c1p2
//2020
#include <bits/stdc++.h>
using namespace std;

int main() { 
    int R;
    cin >> R;
    
    cout << fixed << setprecision(6) << M_PI*pow(R,2) << endl;
    cout << fixed << setprecision(6) << M_PI*pow(R,2)/(M_PI/2.000000000000000) << endl;
    return 0;
}
